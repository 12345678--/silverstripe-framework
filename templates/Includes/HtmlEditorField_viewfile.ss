<div class="ss-htmleditorfield-file $appCategory" data-id="$File.ID" data-url="$URL">
	<div class="overview">
		<span class="thumbnail">$Preview</span>
		<span class="title">$Name</span>
	</div>
	<div class="details">
		<fieldset>
			<% control Fields %>
				$FieldHolder
			<% end_control %>
		</fieldset>
	</div>
</div>